document.getElementById("search").addEventListener("input", function () {
    let q = this.value.toLowerCase();
    let res = contatos.filter(c =>
        c.org.toLowerCase().includes(q) ||
        c.email.toLowerCase().includes(q) ||
        c.tel.toLowerCase().includes(q)
    );

    document.getElementById("results").innerHTML = res.map(c => `
        <div class="item">
            <div class="name">${c.org}</div>
            <div class="telefone"><strong>Telefone:</strong> ${c.tel}</div>
            <div class="email"><strong>Email:</strong> ${c.email}</div>
        </div>
    `).join("");
});
</script>

